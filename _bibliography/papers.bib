doi = {10.1038/s41558-024-02180-2},
file = {:C$\backslash$:/Users/cmned/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Barnard et al. - 2024 - Projections of multiple climate-related coastal hazards for the US Southeast Atlantic.pdf:pdf},
issn = {1758-678X},
journal = {Nature Climate Change},
mendeley-groups = {Stuff that I did for resume / CV},
month = {jan},
number = {1},
pages = {101--109},
title = {{Projections of multiple climate-related coastal hazards for the US Southeast Atlantic}},
url = {https://www.nature.com/articles/s41558-024-02180-2},
volume = {15},
year = {2025}
}
@article{Tang2025,
