file = {:C$\backslash$:/Users/cmned/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Giardino et al. - 2018 - A quantitative assessment of human interventions and climate change on the West African sediment budget.pdf:pdf},
issn = {09645691},
journal = {Ocean {\&} Coastal Management},
mendeley-groups = {SO TC winds,Stuff that I did for resume / CV},
